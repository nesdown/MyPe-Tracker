}
    
if(global.steps &lt;= 0){
    draw_set_font(font_steps);
    draw_text(235, 288, "0");
}    
}

else{
    draw_set_font(font_sleepresults);
    
    if(global.sleepsteps &gt; 0 and global.sleepsteps &lt;= 10){
    draw_text(240, 288, "It looks like you #slept very well #this night. That's #good!");
}
        
    if(global.sleepsteps &gt; 10 and global.sleepsteps &lt;= 30){
    draw_text(240, 288, "You tried to fall #asleep for a long #time and slept #discomposedly.");
}    
    
    if(global.sleepsteps &gt; 30 and global.sleepsteps &lt;= 500){
    draw_text(240, 250, "Maybe you fell #asleep late and #walked while #slept. #Try to go to bed #earlier.");
}

    if(global.sleepsteps &lt;= 0){
    draw_text(240, 288, "There are no #results of your #sleeping.");
}
}
